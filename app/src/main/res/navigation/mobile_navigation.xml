<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.myfirebaseapp.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_navigation_home_to_mostrar"
            app:destination="@id/mostrar" />
    </fragment>

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.myfirebaseapp.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" >
        <action
            android:id="@+id/action_navigation_notifications_to_info_usuario"
            app:destination="@id/info_usuario" />
        <action
            android:id="@+id/action_navigation_notifications_to_plan_usuario"
            app:destination="@id/plan_usuario" />
        <action
            android:id="@+id/action_navigation_notifications_to_recomendaciones_usuario"
            app:destination="@id/recomendaciones_usuario" />
        <action
            android:id="@+id/action_navigation_notifications_to_contacto"
            app:destination="@id/contacto" />
    </fragment>
    <fragment
        android:id="@+id/info_usuario"
        android:name="com.example.myfirebaseapp.info_usuario"
        android:label="info_usuario" />
    <fragment
        android:id="@+id/plan_usuario"
        android:name="com.example.myfirebaseapp.plan_usuario"
        android:label="fragment_plan_usuario"
        tools:layout="@layout/fragment_plan_usuario" />
    <fragment
        android:id="@+id/recomendaciones_usuario"
        android:name="com.example.myfirebaseapp.recomendaciones_usuario"
        android:label="fragment_recomendaciones_usuario"
        tools:layout="@layout/fragment_recomendaciones_usuario" >
        <action
            android:id="@+id/action_recomendaciones_usuario_to_perder_peso2"
            app:destination="@id/perder_peso2" />
        <action
            android:id="@+id/action_recomendaciones_usuario_to_espinacas"
            app:destination="@id/espinacas" />
        <action
            android:id="@+id/action_recomendaciones_usuario_to_aguacate"
            app:destination="@id/aguacate" />
        <action
            android:id="@+id/action_recomendaciones_usuario_to_frijoles2"
            app:destination="@id/frijoles2" />
        <action
            android:id="@+id/action_recomendaciones_usuario_to_pescado2"
            app:destination="@id/pescado2" />
    </fragment>
    <fragment
        android:id="@+id/perder_peso2"
        android:name="com.example.myfirebaseapp.perder_peso"
        android:label="fragment_perder_peso"
        tools:layout="@layout/fragment_perder_peso" >
        <action
            android:id="@+id/action_perder_peso2_to_recomendaciones_usuario"
            app:destination="@id/recomendaciones_usuario" />
        <action
            android:id="@+id/action_perder_peso2_to_aguacate2"
            app:destination="@id/aguacate2" />
        <action
            android:id="@+id/action_perder_peso2_to_pan"
            app:destination="@id/pan" />
        <action
            android:id="@+id/action_perder_peso2_to_aceite2"
            app:destination="@id/aceite2" />
        <action
            android:id="@+id/action_perder_peso2_to_frutos2"
            app:destination="@id/frutos2" />
    </fragment>
    <fragment
        android:id="@+id/espinacas"
        android:name="com.example.myfirebaseapp.espinacas"
        android:label="espinacas" />
    <fragment
        android:id="@+id/aguacate"
        android:name="com.example.myfirebaseapp.aguacate"
        android:label="aguacate" />
    <fragment
        android:id="@+id/frijoles2"
        android:name="com.example.myfirebaseapp.frijoles"
        android:label="frijoles" />
    <fragment
        android:id="@+id/pescado2"
        android:name="com.example.myfirebaseapp.pescado"
        android:label="pescado" />
    <fragment
        android:id="@+id/aguacate2"
        android:name="com.example.myfirebaseapp.aguacate2"
        android:label="aguacate2" />
    <fragment
        android:id="@+id/pan"
        android:name="com.example.myfirebaseapp.pan"
        android:label="pan" />
    <fragment
        android:id="@+id/aceite2"
        android:name="com.example.myfirebaseapp.aceite"
        android:label="aceite" />
    <fragment
        android:id="@+id/frutos2"
        android:name="com.example.myfirebaseapp.frutos"
        android:label="frutos" />
    <fragment
        android:id="@+id/mostrar"
        android:name="com.example.myfirebaseapp.Mostrar"
        android:label="fragment_mostrar"
        tools:layout="@layout/fragment_mostrar" >
        <action
            android:id="@+id/action_mostrar_to_uno"
            app:destination="@id/uno" />
        <action
            android:id="@+id/action_mostrar_to_dos"
            app:destination="@id/dos" />
        <action
            android:id="@+id/action_mostrar_to_tres"
            app:destination="@id/tres" />
        <action
            android:id="@+id/action_mostrar_to_cuatro"
            app:destination="@id/cuatro" />
        <action
            android:id="@+id/action_mostrar_to_cinco"
            app:destination="@id/cinco" />
        <action
            android:id="@+id/action_mostrar_to_seis"
            app:destination="@id/seis" />
        <action
            android:id="@+id/action_mostrar_to_siete"
            app:destination="@id/siete" />
        <action
            android:id="@+id/action_mostrar_to_ocho"
            app:destination="@id/ocho" />
        <action
            android:id="@+id/action_mostrar_to_nueve"
            app:destination="@id/nueve" />
    </fragment>
    <fragment
        android:id="@+id/uno"
        android:name="com.example.myfirebaseapp.Uno"
        android:label="fragment_uno"
        tools:layout="@layout/fragment_uno" />
    <fragment
        android:id="@+id/dos"
        android:name="com.example.myfirebaseapp.Dos"
        android:label="fragment_dos"
        tools:layout="@layout/fragment_dos" />
    <fragment
        android:id="@+id/tres"
        android:name="com.example.myfirebaseapp.Tres"
        android:label="fragment_tres"
        tools:layout="@layout/fragment_tres" />
    <fragment
        android:id="@+id/cuatro"
        android:name="com.example.myfirebaseapp.Cuatro"
        android:label="fragment_cuatro"
        tools:layout="@layout/fragment_cuatro" />
    <fragment
        android:id="@+id/cinco"
        android:name="com.example.myfirebaseapp.Cinco"
        android:label="fragment_cinco"
        tools:layout="@layout/fragment_cinco" />
    <fragment
        android:id="@+id/seis"
        android:name="com.example.myfirebaseapp.Seis"
        android:label="fragment_seis"
        tools:layout="@layout/fragment_seis" />
    <fragment
        android:id="@+id/siete"
        android:name="com.example.myfirebaseapp.Siete"
        android:label="fragment_siete"
        tools:layout="@layout/fragment_siete" />
    <fragment
        android:id="@+id/ocho"
        android:name="com.example.myfirebaseapp.Ocho"
        android:label="fragment_ocho"
        tools:layout="@layout/fragment_ocho" />
    <fragment
        android:id="@+id/nueve"
        android:name="com.example.myfirebaseapp.Nueve"
        android:label="fragment_nueve"
        tools:layout="@layout/fragment_nueve" />
    <fragment
        android:id="@+id/contacto"
        android:name="com.example.myfirebaseapp.Contacto"
        android:label="fragment_contacto"
        tools:layout="@layout/fragment_contacto" />
</navigation>